{
    "manifest_version": 3,
    "name": "LinkedIn People Search Enhancer",
    "version": "1.0",
    "description": "Adds a link to the People tab for direct people search",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/company/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }